<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="UserSqlMap">
    <select id="SelectTESTDB1" resultType="java.util.HashMap">
        SELECT "TEST_DB_TIME",
        "TEST_DB_USE",
        "TEST_DB_COST",
        "TEST_DB_USE" * "TEST_DB_COST" AS "TEST_DB_PRICE"
        FROM "TEST_DB_1"
    </select>
</mapper>